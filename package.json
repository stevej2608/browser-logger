{
  "name": "@holonix/browser-logger",
  "version": "0.0.5",
  "description": "This is a simple utility package",
  "author": "Steve Jones",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevej2608/browser-logger.git"
  },
  "bugs": {
    "url": "https://github.com/stevej2608/browser-logger/issues"
  },
  "keywords": [
    "browser",
    "trace",
    "log",
    "logging"
  ],
  "type": "module",
  "main": "./dist/browser_logger.cjs",
  "module": "./dist/browser_logger.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/browser_logger.js",
      "require": "./dist/browser_logger.cjs"
    }
  },
  "scripts": {
    "dev": "vite --port 3100",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "publish": "npm pack --dry-run",
    "clean": "rm -rf ./dist/*"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-node-polyfills": "^0.9.0"
  },
  "dependencies": {
    "dateformat": "^5.0.3",
    "sprintf-js": "^1.1.2",
    "stacktracey": "^2.1.8"
  }
}
